{
  "extends": "./typedoc.base.json",
  "plugin": ["typedoc-plugin-markdown"],
  "hidePageHeader": true,
  "expandObjects": true,
  "excludeGroups": true,
  "outputFileStrategy": "members",
  "useCodeBlocks": true,
  "indexFormat": "table",
  "parametersFormat": "table",
  "textContentMappings": {
    "breadcrumbs.home": "API",
    "title.indexPage": "API"
  }
}
