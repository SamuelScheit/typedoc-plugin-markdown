{
  "extends": "./typedoc.base.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-remark"],
  "hidePageHeader": true,
  "expandObjects": false,
  "excludeGroups": true,
  "outputFileStrategy": "members",
  "useCodeBlocks": false,
  "expandParameters": false,
  "indexFormat": "table",
  "parametersFormat": "table",
  "entryFileName": "README.md",
  "textContentMappings": {
    "breadcrumbs.home": "API",
    "title.indexPage": "Resources"
  },
  "remarkPlugins": [
    "remark-gfm",
    ["remark-toc", { "skip": "Parameters|Returns" }]
  ],
  "propertiesFormat": "list"
}
