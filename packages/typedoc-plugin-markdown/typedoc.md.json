{
  "extends": "./typedoc.base.json",
  "out": "./plugin-docs",
  "hidePageHeader": true,
  "expandObjects": false,
  "excludeGroups": false,
  "outputFileStrategy": "members",
  "useCodeBlocks": false,
  "expandParameters": false,
  "fileExtension": ".md",
  "textContentMappings": {
    "breadcrumbs.home": "API",
    "title.indexPage": "API",
    "title.memberPage": "{name}"
  },
  "indexFormat": "table",
  "parametersFormat": "table",
  "propertiesFormat": "list",
  "hideBreadcrumbs": false
}
