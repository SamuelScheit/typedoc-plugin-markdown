{
  "compilerOptions": {
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "lib": ["es2020", "dom"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "target": "es2020",
  },
  "include": ["*.ts"],
}
